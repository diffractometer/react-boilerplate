{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "React boilerplate.",
  "main": "index.js",
  "scripts": {
    "start": "parallelshell \"npm run lint:tools\" \"npm run test:watch\" \"npm run open:src\"",
    "open:src": "node bin/devServer.js",
    "build:html": "node bin/buildHtml.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run build:html",
    "lint:tools": "eslint webpack.config.js tools",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Hunter Husar",
  "license": "MIT",
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.1.2",
    "redux": "^3.1.7"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "6.4.5",
    "babel-eslint": "4.1.7",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-plugin-react-transform": "2.0.0",
    "browser-sync": "^2.11.1",
    "cheerio": "0.19.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "file-loader": "0.8.5",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "1.2.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4",
    "babel-preset-react": "6.3.13",
    "object-assign": "4.0.1",
    "watch": "0.17.1",
    "aight": "2.1.1",
    "event-source-polyfill": "0.0.7"
  }
}
